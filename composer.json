{
    "name": "headerx/laravel-iframes",
    "description": "Easily load routes inside an iframe.",
    "keywords": [
        "headerx",
        "laravel",
        "laravel-iframes"
    ],
    "homepage": "https://github.com/headerx/laravel-iframes",
    "license": "MIT",
    "authors": [
        {
            "name": "inmanturbo",
            "email": "inman.turbo@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/contracts": "~8.0|~9.0",
        "illuminate/routing": "~8.0|~9.0",
        "illuminate/support": "~8.0|~9.0",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "friendsofphp/php-cs-fixer": "^3.0",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "HeaderX\\Iframes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HeaderX\\Iframes\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "HeaderX\\Iframes\\IframesServiceProvider"
            ],
            "aliases": {
                "Iframes": "HeaderX\\Iframes\\IframesFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
